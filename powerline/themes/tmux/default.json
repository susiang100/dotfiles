{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "uptime": {
            "before": "⇑  "
        },
        "hostname": {
            "before": "Ⓗ "
        },
        "date": {
            "before": "⌚  "
        }
    },
    "segments": {
        "right": [
            {
                "name": "system_load",
                "priority": 50
            },
            {
                "name": "date",
                "args": {
                    "format": "%Y-%m-%d %H:%M",
                    "istime": true
                }
            },
            {
                "name": "hostname"
            }
        ]
    }
}
